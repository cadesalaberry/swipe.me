{
  "name": "swipe.me",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "deploy": "vue-cli-service s3-deploy"
  },
  "dependencies": {
    "@sentry/browser": "5.15.5",
    "@sentry/integrations": "5.15.5",
    "aws-amplify": "3.0.10",
    "aws-amplify-vue": "2.1.1",
    "axios": "0.19.2",
    "core-js": "3.6.5",
    "current-git-branch": "1.1.0",
    "eslint-plugin-vue": "6.2.2",
    "register-service-worker": "1.7.1",
    "swing": "3.1.4",
    "uuid": "8.0.0",
    "vue": "2.6.11",
    "vue-cli-plugin-s3-deploy": "4.0.0-rc3",
    "vue-material": "1.0.0-beta-13",
    "vue-router": "3.1.6",
    "vue-swing": "0.0.10",
    "vuelidate": "0.7.5",
    "vuex": "3.3.0",
    "vuex-persist": "2.2.0"
  },
  "devDependencies": {
    "@types/graphql": "14.5.0",
    "@types/uuid": "7.0.3",
    "@types/vue-material": "https://github.com/calebsander/vue-material-types",
    "@types/vuelidate": "0.7.13",
    "@types/zen-observable": "0.8.0",
    "@typescript-eslint/eslint-plugin": "2.30.0",
    "@typescript-eslint/parser": "2.30.0",
    "@vue/cli-plugin-babel": "4.3.1",
    "@vue/cli-plugin-eslint": "4.3.1",
    "@vue/cli-plugin-pwa": "4.3.1",
    "@vue/cli-plugin-typescript": "4.3.1",
    "@vue/cli-service": "4.3.1",
    "@vue/eslint-config-standard": "5.1.2",
    "@vue/eslint-config-typescript": "5.0.2",
    "babel-eslint": "10.1.0",
    "eslint": "7.0.0",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-node": "11.0.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.1",
    "lint-staged": "10.2.2",
    "typescript": "3.8.3",
    "vue-gh-pages": "1.19.0",
    "vue-template-compiler": "2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/typescript/recommended",
      "@vue/standard"
    ],
    "rules": {
      "@typescript-eslint/no-var-requires": 0
    },
    "parserOptions": {
      "parser": "@typescript-eslint/parser"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged && ./tools/branch-validator.js"
  },
  "homepage": "git@github.com:cadesalaberry/swipe.me",
  "lint-staged": {
    "*.ts": [
      "vue-cli-service lint"
    ],
    "*.js": [
      "vue-cli-service lint"
    ],
    "*.vue": [
      "vue-cli-service lint"
    ]
  }
}
