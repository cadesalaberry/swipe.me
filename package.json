{
  "name": "swipeme.io",
  "version": "0.1.0",
  "main": "packages/api/src/index.ts",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "client": "yarn workspace @swipeme.io/client serve",
    "deploy": "sls deploy --stage=debug",
    "api": "NODE_ENV=development sls offline start",
    "api:seed:remote": "sls dynamodb seed --online --region eu-west-1",
    "serve": "concurrently --kill-others-on-fail \"yarn api\"  \"yarn client\""
  },
  "dependencies": {
    "@swipeme.io/client": "*",
    "@swipeme.io/common": "*",
    "aws-sdk": "^2.678.0",
    "aws-serverless-express": "^3.3.8",
    "babel-runtime": "^6.26.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "http-status": "^1.4.2",
    "serverless-http": "^2.3.2",
    "uuid": "^8.0.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "8.10.51",
    "@types/aws-serverless-express": "3.3.3",
    "@types/cors": "2.8.6",
    "@types/express": "4.17.6",
    "@types/node": "12.12.39",
    "@types/uuid": "7.0.3",
    "@typescript-eslint/eslint-plugin": "2.33.0",
    "@typescript-eslint/parser": "2.33.0",
    "concurrently": "^5.2.0",
    "current-git-branch": "1.1.0",
    "eslint": "7.0.0",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.1",
    "fork-ts-checker-webpack-plugin": "4.1.4",
    "lint-staged": "10.2.2",
    "serverless": "1.71.1",
    "serverless-domain-manager": "4.0.1",
    "serverless-dynamodb-local": "0.2.39",
    "serverless-finch": "^2.5.2",
    "serverless-offline": "6.1.5",
    "serverless-webpack": "5.3.2",
    "ts-loader": "7.0.4",
    "typescript": "3.9.2",
    "webpack": "4.43.0",
    "webpack-node-externals": "^1.7.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/typescript/recommended",
      "@vue/standard"
    ],
    "rules": {
      "@typescript-eslint/no-var-requires": 0
    },
    "parserOptions": {
      "parser": "@typescript-eslint/parser"
    }
  },
  "gitHooks": {
    "pre-commit": "./packages/tools/branch-validator.js"
  },
  "lint-staged": {
  },
  "homepage": "git@github.com:cadesalaberry/swipe.me"
}
