{
  "name": "swipeme.io",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "client": "yarn workspace @swipeme.io/client serve",
    "api": "NODE_ENV=development sls offline start",
    "serve": "concurrently --kill-others-on-fail \"yarn api\"  \"yarn client\""
  },
  "dependencies": {},
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "2.33.0",
    "@typescript-eslint/parser": "2.33.0",
    "concurrently": "^5.2.0",
    "eslint": "7.0.0",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.1",
    "fork-ts-checker-webpack-plugin": "4.1.4",
    "lint-staged": "10.2.2",
    "serverless": "1.71.1",
    "serverless-domain-manager": "4.0.1",
    "serverless-dynamodb-local": "0.2.39",
    "serverless-finch": "^2.5.2",
    "serverless-offline": "6.1.5",
    "serverless-webpack": "5.3.2",
    "ts-loader": "7.0.4",
    "typescript": "3.9.2",
    "webpack": "4.43.0",
    "webpack-node-externals": "^1.7.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/typescript/recommended",
      "@vue/standard"
    ],
    "rules": {
      "@typescript-eslint/no-var-requires": 0
    },
    "parserOptions": {
      "parser": "@typescript-eslint/parser"
    }
  },
  "gitHooks": {
    "pre-commit": "./packages/tools/branch-validator.js"
  },
  "lint-staged": {
  },
  "homepage": "git@github.com:cadesalaberry/swipe.me"
}
